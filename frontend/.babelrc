{
  "presets": [
    ["env", { "modules": false }],
    "stage-3"
  ],
  "plugins": [
    ["component", [
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]],
    ["transform-runtime", {
      polyfill: false,
      regenerator: true
    }],
    "transform-regenerator",
    "transform-es2015-modules-commonjs",
    "dynamic-import-node"
  ],
  "comments": false,
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}
