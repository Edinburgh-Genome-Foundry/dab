{
  "presets": [
    "es2017",
    "stage-2"
  ],
  "plugins": [
    ["component", [
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-default"
      }
    ],
    "transform-runtime"]
  ],
  "comments": false,
  "env": {
    "test": {
      "plugins": [ "istanbul", "syntax-trailing-function-commas" ]
    }
  }
}
